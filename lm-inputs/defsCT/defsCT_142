}}}"b";"c"};{% \ar^{Q_{1h1}}"c";"d"};{\ar_{\beta_{abc}}"a";"e"};{\ar_{Q_{11h}}"e";"f"};{\ar_{% Q_{f11}}"f";"d"};{\ar^{\beta_{a^{\prime}bc}}"b";"g"};{\ar|{Q_{11h}}"g";"d"};{% \ar_{Q_{f11}}"e";"g"};(55,-35)*+{\cong};{\ar@{=>}^{\beta_{fbc^{\prime}}}(62.5,% -17);(58.5,-20)};{\ar@{=>}^{\beta_{abh}}(47.5,-17);(43.5,-20)};(55,0)*+{P_{abb% }}="1";(70,-7)*+{P_{a^{\prime}bb}}="2";(70,-35)*+{Q_{a^{\prime}c^{\prime}c^{% \prime}}}="3";(55,-42)*+{Q_{a^{\prime}c^{\prime}c}}="4";(40,-7)*+{Q_{acc}}="5"% ;(40,-35)*+{Q_{acc^{\prime}}}="6";(55,-28)*+{Q_{ac^{\prime}c^{\prime}}}="7";{% \ar^{P_{f11}}"1";"2"};{\ar^{\beta_{a^{\prime}bc^{\prime}}}"2";"3"};{\ar^{Q_{1h% 1}}"3";"4"};{\ar_{\beta_{abc}}"1";"5"};{\ar_{Q_{11h}}"5";"6"};{\ar_{Q_{f11}}"6% ";"4"};{\ar|{\beta_{abc}}"1";"7"};{\ar^{Q_{f11}}"7";"3"};{\ar_{Q_{1h1}}"7";"6"% };(-7.5,-23.5)*+{\cong};{\ar@{=>}^{\beta_{fbc}}(1,-4.5);(-3,-8.5)};{\ar@{=>}^{% \beta_{a^{\prime}bh}}(7.5,-22);(3.5,-25)};{\ar@{=}(25.5,-21);(29.5,-21)};\endxy$ for all $b\in\mathcal{B}$. • EP5 For each $a\in\mathcal{A}$, $b\in\mathcal{B}$, and $c\in\mathcal{C}$, $\beta_{a1_{b}c}=id_{\beta_{abc}\cdot P_{abb}},\beta_{ab1_{c}}=id_{Q_{acc}\cdot% \beta_{abc}}.$ • EP6 Given $g,g^{\prime}\colon b\rightarrow b^{\prime}$ and $\gamma\colon g\Rightarrow g^{\prime}$ in $\mathcal{B}$, there is an equality of pasting diagrams $\xy(0,0)*+{P_{ab^{\prime}b}}="a";(30,0)*+{P_{abb}}="b";(0,-18)*+{P_{ab^{\prime% }b^{\prime}}}="c";(30,-18)*+{Q_{acc}}="d";{\ar_{P_{ag^{\prime}b}}"a";"b"};{\ar% _{P_{ab^{\prime}g^{\prime}}}"a";"c"};{\ar^{\beta_{abc}}"b";"d"};{\ar_{\beta_{% ab^{\prime}c}}"c";"d"};{\ar@/^{2}pc/^{P_{agb}}"a";"b"};{\ar@{=>}^{\beta_{ag^{% \prime}c}}(12,-7);(12,-11)};{\ar@{=>}^{P_{a\gamma b}}(12,6);(12,2)};(70,0)*+{P% _{ab^{\prime}b}}="1";(100,0)*+{P_{abb}}="2";(70,-18)*+{P_{ab^{\prime}b^{\prime% }}}="3";(100,-18)*+{Q_{acc}}="4";{\ar^{P_{agb}}"1";"2"};{\ar^{P_{ab^{\prime}g}% }"1";"3"};{\ar^{\beta_{abc}}"2";"4"};{\ar_{\beta_{ab^{\prime}c}}"3";"4"};{\ar@% /_{2}pc/_{P_{ab^{\prime}g^{\pri